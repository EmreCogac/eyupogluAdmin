    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-plugin-transform-ensure-ignore": "^0.1.0",
    "babel-preset-env": "^1.6.0",
    "linklocal": "^2.8.1",
    "react-scripts": "^1.0.14"
